{
  "is_market_upper": true,
  "timestamp_in_seconds": true,
  "latency-test-symbol": "XBT/USD",
  "trades_custom_id": true,
  "rest": {
    "url": "https://api.kraken.com",
    "timeout": 15000,
    "separator": "",
    "error": {
      "key": "error",
      "is_array": true
    },
    "endpoints": {
      "available_pairs": {
        "path": "/0/public/AssetPairs",
        "response": {
          "data_inside": "result",
          "symbol": "altname",
          "status_key": "status",
          "status_active": "online"
        }
      },
      "trades": {
        "path": "/0/public/Trades?pair=<market>&count=1000&since=<since>",
        "response": {
          "newer_first": false,
          "data_inside": "result",
          "get_first_value": true,
          "timestamp": "2",
          "is_buy_key": "3",
          "is_buy_value": "b",
          "price": "0",
          "amount": "1",
          "trade_id_key": "6",
          "pagination": {
            "check_for_newer": true,
            "max_arr_size": 1000,
            "to_del_url": "<since>",
            "to_add_url": "<page_id>",
            "page_id": "2"
          }
        }
      }
    }
  },
  "ws": {
    "url": "wss://ws.kraken.com",
    "timeout": 15000,
    "separator": "/",
    "error": {
      "key": "event",
      "value": "error"
    },
    "subcriptions": {
      "orderbook": {
        "request": "{\"event\":\"subscribe\",\"reqid\":<ws_req_id>,\"pair\":[\"<market>\"],\"subscription\":{\"name\":\"book\",\"depth\":1000}}",
        "response": {
          "id": "reqid",
          "channel_id_key": "channelName",
          "error_key": "status",
          "error_val": "error"
        },
        "snapshot": {
          "reset_avoid_repetition_cache": true,
          "identifier_key": "bs",
          "channel_id_key": "2",
          "asks": "as",
          "bids": "bs"
        },
        "update": {
          "apply_only_since_last_snapshot": true,
          "avoid_each_piece_repetition": true,
          "dont_apply_too_old": true,
          "avoid_repetition": true,
          "data_inside": "1,-2",
          "channel_id_key": "-2",
          "asks": "a",
          "bids": "b",
          "timestamp_in_micro": true,
          "pl": {
            "price": "0",
            "amount": "1",
            "timestamp": "2"
          }
        }
      },
      "trades": {
        "request": "{\"event\":\"subscribe\",\"reqid\":<ws_req_id>,\"pair\":[\"<market>\"],\"subscription\":{\"name\":\"trade\"}}",
        "response": {
          "id": "reqid",
          "channel_id_key": "channelName",
          "error_key": "status",
          "error_val": "error"
        },
        "update": {
          "data_inside": "1",
          "id_should_be_higher": true,
          "channel_id_key": "2",
          "timestamp": "2",
          "is_buy_key": "3",
          "is_buy_value": "b",
          "price": "0",
          "amount": "1"
        }
      }
    },
    "msgs_to_ignore": [
      [ "status", "online" ],
      [ "event", "heartbeat" ]
    ]
  },
  "asset_translation": {
    "XBT": "BTC",
    "XDG": "DOGE"
  }
}