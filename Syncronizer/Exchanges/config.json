{
  "binance-spot": {
    "separator": "",
    "rest": {
      "url": "https://api.binance.com",
      "timeout": 15000,
      "is_market_upper": true,
      "error": {
        "key": "code"
      },
      "endpoints": {
        "available_pairs": {
          "path": "/api/v3/exchangeInfo",
          "response": {
            "data_inside": "symbols",
            "symbol": "symbol",
            "status_key": "status",
            "status_active": "TRADING"
          }
        },
        "orderbook": {
          "path": "/api/v3/depth?symbol=<market>&limit=5000",
          "response": {
            "asks": "asks",
            "bids": "bids",
            "last_update_nonce": "lastUpdateId"
          }
        },
        "trades": {
          "path": "/api/v3/aggTrades?symbol=<market>&startTime=<since>",
          "response": {
            "trade_id_key": "l",
            "newer_first": false,
            "timestamp": "T",
            "is_buy_key": "m",
            "is_buy_value": false,
            "price": "p",
            "amount": "q",
            "pagination": {
              "check_for_newer": true,
              "max_arr_size": 1000,
              "newer_first": false,
              "to_del_url": "&startTime=<since>",
              "to_add_url": "&fromId=<page_id>",
              "page_id": "a"
            }
          }
        }
      }
    },
    "ws": {
      "url": "wss://stream.binance.com:9443/stream",
      "timeout": 15000,
      "is_market_upper": false,
      "error": { 
        "key": "code" 
      },
      "subcriptions": {
        "orderbook": {
          "request": "{\"method\":\"SUBSCRIBE\",\"params\":[\"<market>@depth@100ms\"],\"id\":<ws_req_id>}",
          "response": {
            "id": "id"
          },
          "update": {
            "channel_id_key": "stream",
            "channel_id": "<market>@depth@100ms",
            "data_inside": "data",
            "asks": "a",
            "bids": "b",
            "pl": {
              "price": 0,
              "amount": 1
            },
            "timestamp": "E",
            "first_upd_nonce_key": "U",
            "last_upd_nonce_key": "u"
          }
        },
        "trades": {
          "request": "{\"method\":\"SUBSCRIBE\",\"params\":[\"<market>@trade\"],\"id\":<ws_req_id>}",
          "response": {
            "id": "id"
          },
          "update": {
            "id_should_be_higher": true,
            "trade_id_key": "t",
            "channel_id_key": "stream",
            "channel_id": "<market>@trade",
            "data_inside": "data",
            "receive_separately_trades_as_obj": true,
            "timestamp": "T",
            "is_buy_key": "m",
            "is_buy_value": false,
            "price": "p",
            "amount": "q"
          }
        }
      }
    }
  },
  "kraken-spot": {
    "is_market_upper": true,
    "timestamp_in_seconds": true,
    "trades_custom_id": true,
    "rest": {
      "url": "https://api.kraken.com",
      "timeout": 15000,
      "separator": "",
      "error": {
        "key": "error",
        "is_array": true
      },
      "endpoints": {
        "available_pairs": {
          "path": "/0/public/AssetPairs",
          "response": {
            "data_inside": "result",
            "symbol": "altname",
            "status_key": "status",
            "status_active": "online"
          }
        },
        "trades": {
          "path": "/0/public/Trades?pair=<market>&count=1000&since=<since>",
          "response": {
            "newer_first": false,
            "data_inside": "result",
            "get_first_value": true,
            "timestamp": "2",
            "is_buy_key": "3",
            "is_buy_value": "b",
            "price": "0",
            "amount": "1",
            "trade_id_key": "6",
            "pagination": {
              "check_for_newer": true,
              "max_arr_size": 1000,
              "newer_first": false,
              "to_del_url": "<since>",
              "to_add_url": "<page_id>",
              "page_id": "2"
            }
          }
        }
      }
    },
    "ws": {
      "url": "wss://ws.kraken.com",
      "timeout": 15000,
      "separator": "/",
      "error": {
        "key": "event",
        "value": "error"
      },
      "subcriptions": {
        "orderbook": {
          "request": "{\"event\":\"subscribe\",\"reqid\":<ws_req_id>,\"pair\":[\"<market>\"],\"subscription\":{\"name\":\"book\",\"depth\":1000}}",
          "response": {
            "id": "reqid",
            "channel_id_key": "channelName"
          },
          "snapshot": {
            "identifier_key": "bs",
            "channel_id_key": 2,
            "asks": "as",
            "bids": "bs"
          },
          "update": {
            "data_inside": "1,-2",
            "channel_id_key": -2,
            "asks": "a",
            "bids": "b",
            "pl": {
              "price": 0,
              "amount": 1,
              "timestamp": 2
            }
          }
        },
        "trades": {
          "request": "{\"event\":\"subscribe\",\"reqid\":<ws_req_id>,\"pair\":[\"<market>\"],\"subscription\":{\"name\":\"trade\"}}",
          "response": {
            "id": "reqid",
            "channel_id_key": "channelName"
          },
          "update": {
            "data_inside": 1,
            "channel_id_key": 2,
            "timestamp": 2,
            "is_buy_key": 3,
            "is_buy_value": "b",
            "price": 0,
            "amount": 1
          }
        }
      },
      "msgs_to_ignore": [
        [ "status", "online" ],
        [ "event", "heartbeat" ]
      ]
    }
  },
  "coinbase-spot": {
    "separator": "-",
    "is_market_upper": true,
    "timestamp_ISO_format": true,
    "rest": {
      "url": "https://api.exchange.coinbase.com",
      "timeout": 15000,
      "is_market_upper": true,
      "error": {
        "key": "message"
      },
      "endpoints": {
        "available_pairs": {
          "path": "/products",
          "response": {
            "symbol": "id",
            "status_key": "status",
            "status_active": "online"
          }
        },
        "trades": {
          "path": "/products/<market>/trades",
          "response": {
            "trade_id_key": "trade_id",
            "newer_first": true,
            "timestamp": "time",
            "is_buy_key": "side",
            "is_buy_value": "buy",
            "price": "price",
            "amount": "size",
            "pagination": {
              "newer_first": true,
              "to_add_url": "?after=<page_id>",
              "page_id": "trade_id"
            }
          }
        }
      }
    },
    "ws": {
      "url": "wss://ws-feed.exchange.coinbase.com",
      "timeout": 15000,
      "error": {
        "key": "type",
        "value": "error"
      },
      "auth": {
        "nonce_to_eval": "Date.now() / 1e3",
        "algo": "sha256",
        "secret_buffer_from": "base64",
        "message": "<nonce>GET/users/self/verify",
        "digest_to": "base64",
        "sign_pass": true
      },
      "subcriptions": {
        "orderbook": {
          "request": "{\"type\":\"subscribe\",\"product_ids\":[\"<market>\"],\"channels\":[\"level2\"],\"signature\":\"<signature>\",\"key\":\"<api_key>\",\"passphrase\":\"<api_pass>\",\"timestamp\":\"<sign_nonce>\"}",
          "require_auth": true,
          "response": {
            "acum_list": true,
            "list_id_key": "type",
            "list_id_value": "subscriptions",
            "list_inside": "channels",
            "id": "name",
            "id_value": "level2"
          },
          "snapshot": {
            "channel_id": "snapshot",
            "identifier_key": "type",
            "identifier_value": "snapshot",
            "asks": "asks",
            "bids": "bids",
            "pl": {
              "price": "0",
              "amount": "1"
            }
          },
          "update": {
            "updates_inside": "changes",
            "channel_id_key": "type",
            "channel_id": "l2update",
            "asks_and_bids_together": true,
            "timestamp": "time",
            "pl": {
              "price": "1",
              "amount": "2",
              "is_bids_key": "0",
              "is_bids_value": "buy"
            }
          }
        },
        "trades": {
          "request": "{\"type\":\"subscribe\",\"product_ids\":[\"<market>\"],\"channels\":[\"matches\"]}",
          "response": {
            "acum_list": true,
            "list_inside": "channels",
            "id": "name",
            "id_value": "matches"
          },
          "update": {
            "trade_id_key": "trade_id",
            "receive_separately_trades_as_obj": true,
            "channel_id_key": "type",
            "channel_id": "match",
            "timestamp": "time",
            "is_buy_key": "side",
            "is_buy_value": "buy",
            "price": "price",
            "amount": "size"
          }
        }
      },
      "msgs_to_ignore": [
        [ "type", "last_match" ]
      ]
    }
  }
}