{
  "separator": "",
  "is_market_upper": false,
  "rest": {
    "url": "https://www.bitstamp.net",
    "timeout": 15000,
    "error": {
      "key": "response_code"
    },
    "endpoints": {
      "available_pairs": {
        "path": "/api/v2/trading-pairs-info/",
        "response": {
          "symbol": "url_symbol",
          "status_key": "trading",
          "status_active": "Enabled"
        }
      },
      "trades": {
        "path": "/api/v2/transactions/<market>/?time=minute",
        "response": {
          "trade_id_key": "tid",
          "newer_first": true,
          "timestamp": "date",
          "timestamp_in_seconds": true,
          "is_buy_key": "type",
          "is_buy_value": "0",
          "price": "price",
          "amount": "amount"
        }
      }
    }
  },
  "ws": {
    "url": "wss://ws.bitstamp.net",
    "timeout": 15000,
    "error": {
      
    },
    "subcriptions": {
      "orderbook_snap": {
        "request": "{\"event\":\"bts:subscribe\",\"data\":{\"channel\":\"order_book_<market>\"}}",
        "response": {
          "is_object": true,
          "object_id_key": "event",
          "object_id_value": "bts:subscription_succeeded",
          "id": "channel",
          "id_value": "order_book_<market>",
          "channel_id_key": "channel"
        },
        "update": {
          "channel_id_key": "channel",
          "data_inside": "data",
          "asks": "asks",
          "bids": "bids",
          "pl": {
            "price": "0",
            "amount": "1"
          },
          "timestamp": "microtimestamp",
          "timestamp_in_micro": true,
          "last_upd_nonce_key": "microtimestamp"
        }
      },
      "orderbook": {
        "request": "{\"event\":\"bts:subscribe\",\"data\":{\"channel\":\"diff_order_book_<market>\"}}",
        "response": {
          "is_object": true,
          "object_id_key": "event",
          "object_id_value": "bts:subscription_succeeded",
          "id": "channel",
          "id_value": "diff_order_book_<market>",
          "channel_id_key": "channel"
        },
        "update": {
          "channel_id_key": "channel",
          "data_inside": "data",
          "asks": "asks",
          "bids": "bids",
          "pl": {
            "price": "0",
            "amount": "1"
          },
          "timestamp": "microtimestamp",
          "timestamp_in_micro": true,
          "last_upd_nonce_key": "microtimestamp"
        }
      },
      "trades": {
        "request": "{\"event\":\"bts:subscribe\",\"data\":{\"channel\":\"live_trades_<market>\"}}",
        "response": {
          "id": "event",
          "id_value": "bts:subscription_succeeded",
          "channel_id_key": "channel",
          "channel_id_val": "live_trades_<market>"
        },
        "update": {
          "channel_id_key": "channel",
          "data_inside": "data",
          "receive_separately_trades_as_obj": true,
          "trade_id_key": "id",
          "id_should_be_higher": true,
          "timestamp": "microtimestamp",
          "timestamp_in_micro": true,
          "is_buy_key": "type",
          "is_buy_value": "0",
          "price": "price_str",
          "amount": "amount_str"
        }
      }
    }
  }
}