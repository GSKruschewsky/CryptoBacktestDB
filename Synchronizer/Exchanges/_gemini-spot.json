{
  "separator": "",
  "trades_custom_id": true,
  "rest": {
    "url": "https://api.gemini.com",
    "timeout": 15000,
    "is_market_upper": false,
    "error": {
      "key": "result",
      "value": "error"
    },
    "endpoints": {
      "available_pairs": {
        "path": "/v1/symbols",
        "response": {
          
        }
      },
      "trades": {
        "path": "/v1/trades/<market>?limit_trades=500&timestamp=<since>",
        "response": {
          "trade_id_key": "tid",
          "newer_first": true,
          "timestamp": "timestampms",
          "is_buy_key": "type",
          "is_buy_value": "buy",
          "price": "price",
          "amount": "amount",
          "pagination": {
            "check_for_newer": true,
            "max_arr_size": 500,
            "to_del_url": "&timestamp=<since>",
            "to_add_url": "&since_tid=<page_id>",
            "page_id": "tid"
          }
        }
      }
    }
  },
  "ws": {
    "url": "wss://api.gemini.com/v2/marketdata",
    "timeout": 15000,
    "is_market_upper": true,
    "error": {
      "key": "result",
      "value": "error"
    },
    "subcriptions": {
      "orderbook": {
        "request": "{\"type\":\"subscribe\",\"subscriptions\":[{\"name\":\"l2\",\"symbols\":[\"<market>\"]}]}",
        "response": {
          "include_snapshot": true,
          "is_object": true,
          "object_id_key": "type",
          "object_id_value": "l2_updates",
          "id": "trades",
          "id_value": null,
          "include_trades": true
        },
        "snapshot": {
          "identifier_key": "trades",
          "asks_and_bids_together": true
        },
        "update": {
          "channel_id_key": "type",
          "channel_id": "l2_updates",
          "updates_inside": "changes",
          "asks_and_bids_together": true,
          "pl": {
            "price": "1",
            "amount": "2",
            "is_bids_key": "0",
            "is_bids_value": "buy"
          }
        }
      },
      "trades": {
        "update": {
          "channel_id_key": "type",
          "channel_id": "trade",
          "receive_separately_trades_as_obj": true,
          "timestamp": "timestamp",
          "is_buy_key": "side",
          "is_buy_value": "buy",
          "price": "price",
          "amount": "quantity"
        }
      }
    },
    "msgs_to_ignore": [
      [ "type", "heartbeat" ]
    ]
  }
}